DATASOURCES=KeycloakDS

# postgresql
#
# Created from one of the PostgreSQL templates, e.g. postgresql-ephemeral, using
# the following parameter settings:
#
#    POSTGRESQL_SERVICE_NAME=postgresql
#    POSTGRESQL_USER=joe
#    POSTGRESQL_PASSWORD=user
#    POSTGRESQL_DATABASE=rhsso
#
# See the README.md in the postgresql folder for details on seeding the database.
KeycloakDS_DATABASE=rhsso
KeycloakDS_JNDI=java:jboss/datasources/KeycloakDS
KeycloakDS_DRIVER=postgresql
KeycloakDS_USERNAME=joe
KeycloakDS_PASSWORD=user
KeycloakDS_TX_ISOLATION=TRANSACTION_READ_UNCOMMITTED
KeycloakDS_JTA=true
KeycloakDS_NONXA=true
KeycloakDS_URL=jdbc:postgresql://postgresql-02325498-postgres.apps.spent9.lab.rdu2.cee.redhat.com:80/rhsso
KeycloakDS_SERVICE_HOST=postgresql-02325498-postgres.apps.spent9.lab.rdu2.cee.redhat.com
KeycloakDS_SERVICE_PORT=80
